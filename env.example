# OpenAI API key (required)
OPENAI_API_KEY=
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ANON_KEY=

# Admin API authentication (Basic Auth for POST /api/users)
USERS_API_BASIC_AUTH_ENABLED=true
ADMIN_BASIC_USER=
ADMIN_BASIC_PASS=

# Optionally override the default model used on the client
OPENAI_MODEL=gpt-4o-mini

# Stroop Test Configuration
NEXT_PUBLIC_STROOP_ITI=1000
NEXT_PUBLIC_STROOP_TRIAL_TIMER=5000
NEXT_PUBLIC_STROOP_INSTRUCTION_SWITCH=10

# Session timeout configuration (supports seconds like 300 or minutes like 5m)
# Time after inactivity to display the modal
SESSION_TIMEOUT_WARNING=1m
# Time after the modal is shown to auto-logout
SESSION_TIMEOUT_LOGOUT=5m
# Client-exposed overrides (optional). If set, these take precedence on the client.
NEXT_PUBLIC_SESSION_TIMEOUT_WARNING=
NEXT_PUBLIC_SESSION_TIMEOUT_LOGOUT=